<Application x:Class="UST.Spatial.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:spatial="clr-namespace:UST.Spatial">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="pack://application:,,,/Ccr.MDHybrid;component/MDHybrid/Themes/Generic.xaml"/>
				<ResourceDictionary>
					<spatial:AppBootstrapper x:Key="bootstrapper" />
				</ResourceDictionary>
				<ResourceDictionary Source="GeometryMaps/ak.xaml"/>
				<ResourceDictionary Source="GeometryMaps/dc.xaml"/>
				<ResourceDictionary Source="GeometryMaps/tx.xaml"/>
				<ResourceDictionary Source="Themes/UST.MapViewer.xaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
</Application>
